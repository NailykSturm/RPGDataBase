<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>


<FlowPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lbTitle" text="default" />
      <TextField fx:id="lbDescription" />
      <Button fx:id="editFieldButton" mnemonicParsing="false" text="Edit" />
      <Button fx:id="deleteFieldButton" mnemonicParsing="false" text="Delete" />
   </children>
</FlowPane>
